<Window x:Class="Homework3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock>Enter the US Zip Code or Canadian Postal code below</TextBlock>
        <TextBox x:Name="uxName" TextChanged="uxName_TextChanged"></TextBox>
        <Button x:Name="uxSubmit">Submit</Button>
    </StackPanel>
</Window>
